	<include name="SubMenuList">
		<top>95</top>
		<left>-400</left>
		<bottom>100</bottom>
		<width>400</width>
		<ondown>$PARAM[list_id]</ondown>
		<onup>$PARAM[list_id]</onup>
		<visible>$PARAM[visible]</visible>
		<onleft>9000</onleft>
		<onright>9000</onright>
		<orientation>vertical</orientation>
		<scrolltime tween="cubic" easing="out">500</scrolltime>
		<itemlayout height="100">
			<control type="group">
				<control type="label">
					<left>50</left>
					<right>40</right>
					<height>100</height>
					<label>$INFO[ListItem.Label]</label>
					<align>left</align>
					<aligny>center</aligny>
				</control>
			</control>
		</itemlayout>
		<focusedlayout height="100">
			<control type="group">
				<left>0</left>
				<right>0</right>
				<control type="image">
					<left>0</left>
					<right>25</right>
					<height>100%</height>
					<visible>Control.HasFocus($PARAM[list_id])</visible>
					<texture colordiffuse="primary_color" border="5">buttons/focus.png</texture>
				</control>
				<control type="label">
					<left>50</left>
					<right>45</right>
					<height>100</height>
					<aligny>center</aligny>
					<label>$INFO[ListItem.Label]</label>
					<align>left</align>
					<scroll>true</scroll>
				</control>
			</control>
		</focusedlayout>
	</include>
